#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/f/AIGC/LESSON_HM/ai/openai/gpt4o/pic/node_modules/.pnpm/openai@4.98.0/node_modules/openai/bin/node_modules:/mnt/f/AIGC/LESSON_HM/ai/openai/gpt4o/pic/node_modules/.pnpm/openai@4.98.0/node_modules/openai/node_modules:/mnt/f/AIGC/LESSON_HM/ai/openai/gpt4o/pic/node_modules/.pnpm/openai@4.98.0/node_modules:/mnt/f/AIGC/LESSON_HM/ai/openai/gpt4o/pic/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/f/AIGC/LESSON_HM/ai/openai/gpt4o/pic/node_modules/.pnpm/openai@4.98.0/node_modules/openai/bin/node_modules:/mnt/f/AIGC/LESSON_HM/ai/openai/gpt4o/pic/node_modules/.pnpm/openai@4.98.0/node_modules/openai/node_modules:/mnt/f/AIGC/LESSON_HM/ai/openai/gpt4o/pic/node_modules/.pnpm/openai@4.98.0/node_modules:/mnt/f/AIGC/LESSON_HM/ai/openai/gpt4o/pic/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cli" "$@"
else
  exec node  "$basedir/../../bin/cli" "$@"
fi
